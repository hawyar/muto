{
  "name": "muto",
  "version": "1.0.0",
  "description": "Run ad-hoc SQL query on CSV and JSON",
  "author": "hawyar",
  "main": "dist/muto.js",
  "module": "dist/muto.mjs",
  "type": "module",
  "bin": {
    "muto": "bin/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/muto.mjs",
      "require": "./dist/muto.cjs"
    }
  },
  "types": "types/muto.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "node bin/build.js && tsc",
    "dev": "npm run build && npm run test",
    "test": "tap --no-coverage --reporter=specy -J --debug",
    "fix": "npx ts-standard --fix && npx standard --fix ./tests/**/*.js"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^17.0.23",
    "@types/npm-paths": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "dotenv": "^16.0.0",
    "download": "^8.0.0",
    "esbuild": "^0.14.14",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^7.18.0",
    "husky": "^7.0.0",
    "picocolors": "^1.0.0",
    "tap": "^15.1.6",
    "ts-standard": "^11.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "arg": "^5.0.1",
    "pgsql-parser": "^13.2.0"
  },
  "ts-standard": {
    "ignore": [
      "dist",
      "docs",
      "bin",
      "tests",
      "types"
    ]
  },
  "license": "MIT",
  "standard": {
    "ignore": [
      "dist",
      "docs",
      "lib",
      "types"
    ]
  }
}
