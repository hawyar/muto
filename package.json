{
  "name": "muto",
  "version": "1.0.0",
  "author": "hawyar",
  "license": "MIT",
  "main": "dist/muto.js",
  "module": "dist/muto.mjs",
  "type": "module",
  "bin": {
    "muto": "bin/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/muto.mjs",
      "require": "./dist/muto.js"
    }
  },
  "types": "types/muto.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "node bin/build.js && tsc",
    "dev": "npm run build && npm run test",
    "test": "tap --no-coverage --reporter=specy -J"
  },
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules",
      "/docs"
    ]
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.47.0",
    "@aws-sdk/credential-providers": "^3.47.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/cacache": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "dotenv": "^16.0.0",
    "download": "^8.0.0",
    "esbuild": "^0.14.14",
    "esbuild-node-externals": "^1.4.1",
    "eslint": "^7.18.0",
    "husky": "^7.0.0",
    "tap": "^15.1.6",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "arg": "^5.0.1",
    "atomically": "^1.7.0",
    "pgsql-parser": "^13.2.0",
    "vfile": "^5.3.0"
  }
}
